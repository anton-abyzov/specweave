{
  "name": "specweave-test",
  "version": "1.0.0",
  "description": "Test plugin for unit tests",
  "author": "SpecWeave Team",
  "license": "MIT",
  "specweave_core_version": ">=0.4.0",
  "auto_detect": {
    "files": ["test/"],
    "packages": ["test-package"],
    "env_vars": ["TEST_VAR"]
  },
  "provides": {
    "skills": ["test-skill"],
    "agents": ["test-agent"],
    "commands": ["test-command"]
  },
  "triggers": ["test", "testing", "unit test"]
}
