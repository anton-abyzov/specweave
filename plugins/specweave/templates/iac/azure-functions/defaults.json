{
  "location": "eastus",
  "functionName": "my-azure-function",
  "runtime": "node",
  "runtimeVersion": "20",
  "osType": "linux",
  "skuTier": "Dynamic",
  "skuSize": "Y1",
  "memorySize": 128,
  "timeout": 300,
  "environment": "dev",
  "databaseName": "my-cosmosdb",
  "primaryKey": "id",
  "apiName": "my-api",
  "logRetentionDays": 90,
  "corsOrigins": ["*"],
  "projectName": "my-project",
  "resourceGroupName": "my-resource-group",
  "storageAccountName": "mystorageaccount",
  "cosmosDbThroughput": 400,
  "enableApplicationInsights": true,
  "enableZoneRedundancy": false,
  "enableBackup": false,
  "enableVnet": false,
  "enableManagedIdentity": true
}
