{
  "platform": "supabase",
  "region": "us-east-1",
  "selfHosted": false,
  "enableAuth": true,
  "enableStorage": true,
  "enableRealtime": true,
  "enablePostgis": false,
  "enableVectors": false,
  "database": {
    "version": "15",
    "extensions": ["uuid-ossp", "pgcrypto"],
    "maxConnections": {
      "dev": 50,
      "staging": 100,
      "prod": 200
    }
  },
  "auth": {
    "providers": ["email"],
    "emailConfirmRequired": {
      "dev": false,
      "staging": false,
      "prod": true
    },
    "passwordMinLength": {
      "dev": 8,
      "staging": 10,
      "prod": 12
    },
    "jwtExpiry": {
      "dev": 86400,
      "staging": 7200,
      "prod": 3600
    }
  },
  "storage": {
    "publicBucket": {
      "name": "public",
      "public": true,
      "fileSizeLimit": {
        "dev": 52428800,
        "staging": 20971520,
        "prod": 10485760
      }
    },
    "privateBucket": {
      "name": "private",
      "public": false,
      "fileSizeLimit": {
        "dev": 52428800,
        "staging": 20971520,
        "prod": 10485760
      }
    }
  },
  "api": {
    "maxRows": {
      "dev": 100000,
      "staging": 10000,
      "prod": 1000
    }
  },
  "plan": {
    "dev": "free",
    "staging": "free",
    "prod": "pro"
  }
}
